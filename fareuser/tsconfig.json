{
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es2017"
    ],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
